{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id" : "collections/primitiveArray.schema.json",
  "title": "PrimitiveArray",
  "description": "A container for array primitives",
  "type": "object",
  "properties": {
    "strings": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "booleans": {
      "type": "array",
      "items": {
        "type": "boolean"
      }
    },
    "numbers": {
      "type": "array",
      "items": {
        "type": "number"
      }
    },
    "longs": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    }
  }
}
